{
  "name": "docassemble-IntegratedTesting",
  "version": "0.1.0",
  "description": "Docassembly Line integrated testing development",
  "main": "",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo 'These tests are fake. Use `local`' && mocha tests --timeout 10000",
    "cucumber": "./node_modules/.bin/cucumber-js tests/features/*.feature",
    "broken-cucumber": "./node_modules/.bin/cucumber-js ./node_modules/.bin/docassemble-cucumber/index.js tests/features/*.feature",
    "setup": "node_modules/.bin/da-setup",
    "takedown": "node -e 'require(\"./tests/install-branch.js\").takedown()'",
    "local": "npm run setup && npm run cucumber; npm run takedown"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SuffolkLITLab/docassemble-IntegratedTesting.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SuffolkLITLab/docassemble-IntegratedTesting/issues"
  },
  "homepage": "https://github.com/SuffolkLITLab/docassemble-IntegratedTesting#readme",
  "dependencies": {
    "chai": "^4.2.0",
    "cucumber": "^6.0.5",
    "docassemble-cucumber": "^0.1.11",
    "dotenv": "^8.2.0",
    "mocha": "^7.2.0",
    "puppeteer": "^3.1.0"
  }
}
